<?xml version='1.0' encoding='utf-8'?>
<report name="项目分红明细" fileName="项目分红明细">
	<report-header>
		<column name="项目编号" mapping="loanNo" />
		<column name="项目名称" mapping="loanName" />
		<column name="投资编号" mapping="investNo" />
		<column name="投资人" mapping="investor" />
		<column name="投资金额(元)" mapping="investAmt" />
		<column name="收款人" mapping="getUser" />
		<column name="分红时间" mapping="assignTime" datatype="date"
			format="yyyy-MM-dd HH:mm:ss" />
		<column name="分红金额(元)" mapping="assignAmt" />
		<column name="奖励类型" mapping="rewardTypeName" />
		<column name="付款人" mapping="payUserName" />
		<column name="分红次数" mapping="bonusNum" />
	</report-header>
	<dataSource serviceClassId="rewardAssignService" serviceMethod="getPageListImport" />
</report>
